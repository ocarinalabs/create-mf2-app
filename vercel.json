{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/_mintlify/:path*",
      "destination": "https://ocarinalabs.mintlify.app/_mintlify/:path*"
    },
    {
      "source": "/api/request",
      "destination": "https://ocarinalabs.mintlify.app/_mintlify/api/request"
    },
    {
      "source": "/docs",
      "destination": "https://ocarinalabs.mintlify.app/"
    },
    {
      "source": "/docs/llms.txt",
      "destination": "https://ocarinalabs.mintlify.app/llms.txt"
    },
    {
      "source": "/docs/llms-full.txt",
      "destination": "https://ocarinalabs.mintlify.app/llms-full.txt"
    },
    {
      "source": "/docs/sitemap.xml",
      "destination": "https://ocarinalabs.mintlify.app/sitemap.xml"
    },
    {
      "source": "/docs/robots.txt",
      "destination": "https://ocarinalabs.mintlify.app/robots.txt"
    },
    {
      "source": "/docs/mcp",
      "destination": "https://ocarinalabs.mintlify.app/mcp"
    },
    {
      "source": "/docs/:path*",
      "destination": "https://ocarinalabs.mintlify.app/:path*"
    },
    {
      "source": "/mintlify-assets/:path+",
      "destination": "https://ocarinalabs.mintlify.app/mintlify-assets/:path+"
    }
  ]
}
